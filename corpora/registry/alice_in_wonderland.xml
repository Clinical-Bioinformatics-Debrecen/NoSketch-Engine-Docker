  <maintainer>SOLONGONN</maintainer>

  <vertical>
    <file path="../alice_in_wonderland/alice_in_wonderland.vert" />
    <columns>
      <column name="word" attribute="word"/>
      <column name="lemma" attribute="lemma"/>
      <column name="upos" attribute="upos"/>
